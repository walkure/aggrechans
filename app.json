{
	"name": "aggrechans",
	"description": "Aggregate all Slack messages generated by human.",
	"keywords": [
		"Slack"
	],
	"website": "https://github.com/walkure/aggrechans",
	"env": {
		"SLACK_BOT_TOKEN": {
			"description": "Slack Bot Token(xoxb-)",
			"value": ""
		},
		"SLACK_SIGNING_SECRET": {
			"description": "Signing Secret generated from App Credentials",
			"value": ""
		},
		"AGGREGATE_CHANNEL_ID": {
			"description": "Aggregate destination channel id",
			"required": false
		},
		"DISPATCH_CHANNEL": {
			"description": "Message Dispatch Rules",
			"required": false
		}

	},
	"addons": [{
		"plan": "heroku-redis:hobby-dev",
		"options": {
			"maxmemory_policy": "allkeys-lru",
			"timeout": "60"
		}
	}]
}